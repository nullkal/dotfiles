set t_Co=256

let g:hybrid_use_Xresources = 1
set background=dark
colorscheme hybrid

set title
set showtabline=2
set laststatus=2

set showcmd
set cmdheight=1

set ruler
set number

"set cursorline
set cursorcolumn

set wrap
set display+=lastline

set list
set listchars=eol:$,tab:>-

syntax on
set showmatch
